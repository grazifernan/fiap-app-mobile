<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fiap.com.br.fiapapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FiapApp">
        <activity
            android:name=".view.Alteracao"
            android:exported="false" />
        <activity
            android:name=".view.Lista"
            android:exported="true" />
        <activity
            android:name=".view.Registro"
            android:exported="true" />
        <activity
            android:name=".view.Consulta"
            android:exported="true" />
        <activity
            android:name=".view.Menu"
            android:exported="true" />
        <activity
            android:name=".view.Cadastro"
            android:exported="true" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>